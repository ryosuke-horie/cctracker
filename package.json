{
  "name": "cctracker",
  "version": "0.1.0",
  "description": "Claude Code Rate Limit Tracker - Real-time monitoring tool for Claude AI token usage",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "cctracker": "./dist/cli.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "watch": "tsc --watch",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format src/",
    "format:check": "biome format src/",
    "format:fix": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "keywords": [
    "claude",
    "ai",
    "rate-limit",
    "monitoring",
    "cli",
    "tracker"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-spinners": "^3.0.0",
    "commander": "^14.0.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.1.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.1",
    "@types/node": "^24.0.13",
    "@vitest/coverage-v8": "^3.2.4",
    "husky": "^9.1.7",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vitest": "^3.2.4"
  }
}
