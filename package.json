{
  "name": "cctracker",
  "version": "0.1.0",
  "description": "Claude Code Rate Limit Tracker - Real-time monitoring tool for Claude AI token usage",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "cctracker": "./dist/cli.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "watch": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "claude",
    "ai",
    "rate-limit",
    "monitoring",
    "cli",
    "tracker"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-spinners": "^3.0.0",
    "commander": "^12.0.0",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "eslint": "^8.57.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vitest": "^2.0.3"
  }
}